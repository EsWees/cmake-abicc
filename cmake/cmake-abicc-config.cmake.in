@PACKAGE_INIT@

IF(NOT ABIComplianceChecker_FOUND)
	LIST(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")
	UNSET(@PROJECT_NAME@_FindABIComplianceChecker)
	IF(@PROJECT_NAME@_FIND_REQUIRED)
		LIST(APPEND @PROJECT_NAME@_FindABIComplianceChecker REQUIRED)
	ENDIF()
	IF(@PROJECT_NAME@_FIND_QUIETLY)
		LIST(APPEND @PROJECT_NAME@_FindABIComplianceChecker QUIET)
	ENDIF()
	FIND_PACKAGE(ABIComplianceChecker ${@PROJECT_NAME@_FindABIComplianceChecker})
	IF(NOT ABIComplianceChecker_FOUND)
		SET(@PROJECT_NAME@_FOUND FALSE)
		RETURN()
	ENDIF()
ENDIF()

INCLUDE("${CMAKE_CURRENT_LIST_DIR}/cmake-abicc.cmake")

MESSAGE(STATUS "Found @PROJECT_NAME@: ${CMAKE_CURRENT_LIST_DIR}"
	" (found version \"${@PROJECT_NAME@_VERSION}\")")
